{
	"name": "josefglatz/toolbar-clear-processedfiles",
	"type": "typo3-cms-extension",
	"description": "TYPO3 backend toolbar button to clear processed files on local storages",
	"keywords": [
		"TYPO3",
		"extension",
		"productivity",
		"developmentenvironment"
	],
	"license": "GPL-2.0-only",
	"require": {
		"typo3/cms-core": "^9.5",
		"php": ">=7.2"
	},
	"replace": {
		"typo3-ter/toolbar-clear-processedfiles": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JosefGlatz\\ToolbarClearProcessedfiles\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "toolbar_clear_processedfiles"
		}
	}
}
